$(".navbar-header").click(function() {
	if ($("body").hasClass("mini-navbar")) {
		$("<%= linechartid %>").sparkline(<%= data30d %>, {
			type: 'line',
			width: '110%',
			height: '60',
			lineColor: '#1ab394',
			fillColor: "#ffffff"
		}); 
	}	else {
		$("<%= linechartid %>").sparkline(<%= data30d %>, {
			type: 'line',
			width: '90%',
			height: '60',
			lineColor: '#1ab394',
			fillColor: "#ffffff"
		});
	} 
});

$(window).resize(function() {
	$("<%= linechartid %>").sparkline(<%= data30d %>, {
		type: 'line',
		width: '100%',
		height: '60',
		lineColor: '#1ab394',
		fillColor: "#ffffff"
	});
});

$("<%= linechartid %>").sparkline(<%= data30d %>, {
	type: 'line',
	width: '100%',
	height: '60',
	lineColor: '#1ab394',
	fillColor: "#ffffff"
});